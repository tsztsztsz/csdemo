#include "platform.h"


#include <unistd.h>		/* for usleep() */
#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <errno.h>
#include <assert.h>
#include <stdbool.h>

#include "primes.h"

int main(int argc, char **argv) {

    init_platform();


	printf("Demo application to trace\n");

	calculatePrimes();

	prinr("End of Demo application\n");
    cleanup_platform();
    return EXIT_SUCCESS;
}

